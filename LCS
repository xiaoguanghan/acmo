#include <iostream>
#include <set>
#include <algorithm>
#include <cstdio>
#include <cstring>
using namespace std;
int a[105][105];
char c[105],d[105];
int main()
{
    int n;
    cin>>c;
    cin>>d;
    int lenc = strlen(c),lend = strlen(d);
    for (int i =0;i<lenc;i++)
        for(int j= 0;j<lend;j++)
            a[i+1][j+1] =  max( a[i][j+1],max(a[i+1][j],(c[i]==d[j])?(a[i][j]+1):0));
    cout << a[lenc][lend]<<endl;
    return 0;
}
